{
  "name": "moderation-bot",
  "version": "0.1.0",
  "main": "index.ts",
  "scripts": {
    "build": "tsup src/index.ts --minify",
    "start": "bun run dist/index.js",
    "dev": "tsup src/index.ts --watch --onSuccess \"bun run dist/index.js\"",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\""
  },
  "keywords": [
    "Moderation",
    "Discord",
    "Bot",
    "Safe",
    "Fun"
  ],
  "author": "Zalaya",
  "license": "ISC",
  "description": "Powerful Discord Bot designed to efficiently moderate multiple Discord servers.",
  "dependencies": {
    "discord.js": "^14.16.2"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  }
}
