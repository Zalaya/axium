{
  "name": "moderation-bot",
  "version": "0.1.0",
  "main": "index.ts",
  "scripts": {
    "build": "tsup src/index.ts --minify",
    "start": "node --env-file .env dist/index.js",
    "start:watch": "nodemon",
    "clean": "rimraf node_modules/ package-lock.json dist/",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "install:production": "npm install --omit=dev",
    "install:development": "npm install --include=dev"
  },
  "keywords": [
    "Moderation",
    "Discord",
    "Bot",
    "Safe",
    "Fun"
  ],
  "author": "Zalaya",
  "license": "ISC",
  "description": "Powerful Discord Bot designed to efficiently moderate multiple Discord servers.",
  "dependencies": {
    "discord.js": "^14.16.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  }
}
